{

  "manifest_version" : 2,
  "name" : "Time Pop up",
  "version": "1.0",
  "description" : "This is a simple extension that displays time.Written by <Kevin Muhia archerkreiger@gmial.com/>",
  "content_scripts" : [
    {
      "matches" : ["<all_urls>"],
      "js" : ["content.js"]
    }
  ],
  "browser_action" :{
    "default_icon" : "time-icon.png",
    "default_popup" : "Time.html",
    "default_title" : "Check Time"
  }

}
